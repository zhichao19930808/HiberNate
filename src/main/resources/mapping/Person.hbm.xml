<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="com.ed.action.entity.mapping_m2m.Person" table="xml_person" >
		<id name="id" column="person_id">
			<generator class="uuid"/>
		</id>
		<property name="name" column="person_name"/>
		<property name="time" column="person_time"/>
		<set name="books" cascade="save-update" lazy="true">
            <key column="master_id"/>
			<one-to-many class="com.ed.action.entity.mapping_m2m.Books"/>
		</set>
	</class>

</hibernate-mapping>
